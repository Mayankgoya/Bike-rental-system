<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServelet</servlet-name>
        <servlet-class>com.project.servelet.RegisterServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServelet</servlet-name>
        <servlet-class>com.project.servelet.LoginServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServele</servlet-name>
        <servlet-class>com.project.servelet.LogoutServele</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentServelet</servlet-name>
        <servlet-class>com.project.servelet.RentServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBike</servlet-name>
        <servlet-class>com.project.servelet.DeleteBike</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>com.project.servelet.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateBike</servlet-name>
        <servlet-class>com.project.servelet.UpdateBike</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegister</servlet-name>
        <servlet-class>com.project.servelet.AdminRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServelet</servlet-name>
        <url-pattern>/RegisterServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServelet</servlet-name>
        <url-pattern>/LoginServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServele</servlet-name>
        <url-pattern>/LogoutServele</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentServelet</servlet-name>
        <url-pattern>/RentServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBike</servlet-name>
        <url-pattern>/DeleteBike</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateBike</servlet-name>
        <url-pattern>/UpdateBike</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegister</servlet-name>
        <url-pattern>/AdminRegister</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
